<h1>Users List</h1>

<table>
	<thead>
		<tr>
			<th>User</th>
			<th>Password</th>
		</tr>
	</thead>
	<tbody>
		<% @users.each do | user | %>
		<tr>
			<td><%= user.name %></td>
			<td><%= user.password %></td>
		</tr>
		<% end %>
	</tbody>

</table>

<%= link_to "Create User", "users/new", class: "btn-create-user" %>