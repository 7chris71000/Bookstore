
<div class="container">

	<table>
		<tr>
			<td>Name</td>
			<td><%= @profile.name %></td>
		</tr>
		<tr>
			<td>Age</td>
			<td><%= @profile.age %></td>
		</tr>	
		<tr>
			<td>Gender</td>
			<td><%= @profile.gender %></td>
		</tr>	
		<tr>
			<td>Email</td>
			<td><%= @profile.email %></td>
		</tr>	
	</table>

	<% if(current_user.id == @profile.id) %>
		<%= link_to "Edit", edit_user_registration_path %>
	<% end %>

</div>
